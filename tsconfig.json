{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": false,
        "module": "CommonJS",
        "moduleResolution": "Node",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "types": ["@playwright/test"]
    },
    "include": ["**/*.ts"]
}
